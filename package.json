{
  "name": "vite-plugin-md",
  "type": "module",
  "version": "0.19.0",
  "packageManager": "pnpm@7.1.7",
  "description": "Markdown for Vite",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/vite-plugin-md",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/vite-plugin-md"
  },
  "bugs": "https://github.com/antfu/vite-plugin-md/issues",
  "keywords": [
    "vite",
    "vite-plugin",
    "markdown-it"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --clean --external @rollup/pluginutils",
    "watch": "tsup src/index.ts --dts --format cjs,esm --watch",
    "dev": "npm run build -- --watch",
    "example:build": "cp README.md example/README.md && npm -C example run build",
    "example:dev": "cp README.md example/README.md && npm -C example run dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "audit:fix": "pnpm audit --fix && pnpm install",
    "release": "bumpp",
    "prepublishOnly": "pnpm install",
    "publish": "bumpp",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:update": "vitest -u",
    "test:component": "npx cypress open-ct -p 4000"
  },
  "peerDependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "@vitejs/plugin-vue": "^3.0.1"
  },
  "dependencies": {
    "@yankeeinlondon/code-builder": "1.0.0",
    "@yankeeinlondon/happy-wrapper": "^2.4.0",
    "@yankeeinlondon/link-builder": "^1.0.0",
    "@yankeeinlondon/meta-builder": "^1.0.0",
    "gray-matter": "github:yankeeinlondon/gray-matter",
    "happy-dom": "^6.0.4",
    "markdown-it": "^13.0.1",
    "pathe": "^0.3.2",
    "source-map-js": "^1.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.17.2",
    "@antfu/utils": "^0.5.2",
    "@rollup/pluginutils": "^4.2.1",
    "@type-challenges/utils": "^0.1.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.0.6",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitest/ui": "^0.18.1",
    "@vue/runtime-core": "^3.2.37",
    "@vue/test-utils": "^2.0.2",
    "@vueuse/core": "^8.9.4",
    "bumpp": "^8.2.1",
    "c8": "^7.12.0",
    "callsites": "^4.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "fp-ts": "^2.12.1",
    "npm-run-all": "^4.1.5",
    "picomatch": "^2.3.1",
    "rollup": "^2.77.0",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.2",
    "vitest": "^0.18.1",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2"
  }
}
